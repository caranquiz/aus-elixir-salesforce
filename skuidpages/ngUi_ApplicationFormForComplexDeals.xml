            var newBIRow = biModel.createRow({ });
        }
    });
}else{
    if(!(ClonedAccountModel.data &amp;&amp; ClonedAccountModel.data.length &gt; 0)){
        var newPSRow = ClonedAccountModel.createRow({ });
        var newBIRow = biModel.createRow({ });
    } 
}</jsitem>
