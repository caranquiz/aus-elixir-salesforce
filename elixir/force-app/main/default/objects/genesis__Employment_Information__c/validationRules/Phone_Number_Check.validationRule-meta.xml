<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Phone_Number_Check</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(genesis__Phone__c,&quot;([0-9])*&quot;))</errorConditionFormula>
    <errorMessage>Telephone Number should contain only number.</errorMessage>
</ValidationRule>
