<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Bank_Account_Number_Length</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(REGEX( Bank_Account_Number__c ,"([0-9]){0,9}")), NOT(ISBLANK( Bank_Account_Number__c)))</errorConditionFormula>
    <errorMessage>Bank Account Number can only contain numbers and length cannot be greater than 9</errorMessage>
</ValidationRule>