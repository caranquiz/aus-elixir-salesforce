<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Bank_Account_Name_Change_Length</fullName>
    <active>true</active>
    <errorConditionFormula>OR(LEN( Bank_Account_Name_Change__c ) &gt; 32, ISBLANK(Bank_Account_Name_Change__c ))</errorConditionFormula>
    <errorMessage>Bank Account Name Change length cannot exceed 32 and cannot be blank.</errorMessage>
</ValidationRule>
