<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Middle_Name_Check</fullName>
    <active>false</active>
    <errorConditionFormula>AND(NOT(ISBLANK(MiddleName)),NOT(REGEX(MiddleName,&quot;([a-zA-Z ])*&quot;)))</errorConditionFormula>
    <errorMessage>Middle name can only contain letters</errorMessage>
</ValidationRule>
