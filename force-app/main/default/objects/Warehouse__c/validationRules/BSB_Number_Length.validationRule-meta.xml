<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BSB_Number_Length</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(REGEX(BSB_Number__c ,&quot;([0-9]){6}&quot;)), NOT(ISBLANK( BSB_Number__c)))</errorConditionFormula>
    <errorMessage>BSB Number Should Consists of Six Digits.</errorMessage>
</ValidationRule>
