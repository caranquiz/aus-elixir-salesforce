<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Bank_Account_Name_Length</fullName>
    <active>true</active>
    <errorConditionFormula>OR(LEN(clcommon__Bank_Account_Name__c) &gt; 32, ISBLANK(clcommon__Bank_Account_Name__c))</errorConditionFormula>
    <errorMessage>Bank Account Name length cannot exceed 32 and cannot be blank.</errorMessage>
</ValidationRule>
