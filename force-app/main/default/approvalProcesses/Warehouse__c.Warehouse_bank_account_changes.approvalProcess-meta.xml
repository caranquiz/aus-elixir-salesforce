<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <type>allInternalUsers</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
        <field>Collection_Bank_Account_Name__c</field>
        <field>Collection_Account_Name_Change__c</field>
        <field>Collection_BSB_Number__c</field>
        <field>Collection_Account_BSB_Change__c</field>
        <field>Collection_Bank_Account_Number__c</field>
        <field>Collection_Account_Number_Change__c</field>
        <field>Bank_Account_Name__c</field>
        <field>Disbursal_Bank_Name_Change__c</field>
        <field>BSB_Number__c</field>
        <field>Disbursal_Bank_BSB_Change__c</field>
        <field>Bank_Account_Number__c</field>
        <field>Disbursal_Bank_Account_Change__c</field>
        <field>Trust_Account_Name__c</field>
        <field>Trust_Bank_Name_Change__c</field>
        <field>Trust_BSB_Number__c</field>
        <field>Trust_Bank_BSB_Change__c</field>
        <field>Trust_Account_Number__c</field>
        <field>Trust_Bank_Number_Change__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Warehouse_bank_account_approvals</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Warehouse bank approvals</label>
        <name>Warehouse_bank_approvals</name>
    </approvalStep>
    <emailTemplate>cnotify__Notification_Templates/Warehouse_bank_approvals</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <finalApprovalActions>
        <action>
            <name>Collections_Account_Name_Change</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Collections_Account_Number_Change</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Collections_BSB_Change</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Disbursal_Account_BSB_Change</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Disbursal_Account_Name_Change</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Disbursal_Account_Number_Change</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Trust_Account_BSB_Change</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Trust_Account_Name_Change</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Trust_Account_Number_Change</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Warehouse_Bank_Change_Approval</name>
            <type>Alert</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Bank_Account_Change_Rejection</name>
            <type>Alert</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <label>Warehouse bank account changes</label>
    <recordEditability>AdminOnly</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
